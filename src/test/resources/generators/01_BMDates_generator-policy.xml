<?xml version="1.0" encoding="UTF-8"?><generator_policy>

<generator name="PersistentIdentifier" prefix="han">
    <pattern>{persistentid}</pattern>
</generator>
<generator name="PersistentIdentifierPlus" prefix="han">
    <pattern>{persistentid}/{concept}</pattern>
</generator>
<generator name="PersistentIdentifierPlusPosition" prefix="han">
    <pattern>{persistentid}/{concept}/{position}</pattern>
</generator>

<generator name="PersistentIdentifierPlusPositionPlus" prefix="han">
    <pattern>{persistentid}/{concept}/{position}/{concept1}</pattern>
</generator>
<generator name="PersistentIdentifierPlusPositionPlusPlus" prefix="han">
    <pattern>{persistentid}/{concept}/{position}/{concept1}{concept2}</pattern>
</generator>

<generator name="PersistentIdentifierPlusPlus" prefix="han">
    <pattern>{persistentid}/{concept}/{concept1}</pattern>
</generator>

<generator name="PersistentIdentifierPlusPlusPosition" prefix="han">
    <pattern>{persistentid}/{concept}/{concept1}{position}</pattern>
</generator>

<generator name="PersistentIdentifierPlusPlusPositionPlus" prefix="han">
    <pattern>{persistentid}/{concept}/{concept1}{position}{concept2}</pattern>
</generator>

<generator name="BMdates">
        <custom generatorClass="gr.forth.BMdates">
            <set-arg name="bound" type="constant"/>
            <set-arg name="text"/>
        </custom>
</generator>

<generator name="BasicURI" prefix="rma">
    <pattern>{id}/{concept}/{reference}</pattern>
</generator>

<generator name="BasicURIPlus" prefix="rma">
    <pattern>{id}/{concept}/{reference}/{concept1}</pattern>
</generator>

<generator name="BasicURIPlusPlus" prefix="rma">
    <pattern>{id}/{concept}/{reference}/{concept1}/{concept2}</pattern>
</generator>

<generator name="BasicURIPlusPosition" prefix="rma">
    <pattern>{id}/{concept}/{reference}/{concept1}/{position}</pattern>
</generator>

<generator name="BasicURIPlusPlusPosition" prefix="rma">
    <pattern>{id}/{concept}/{reference}/{concept1}/{concept2}{position}</pattern>
</generator>
<generator name="BasicURIPlusPlusPositionPlus" prefix="rma">
    <pattern>{id}/{concept}/{reference}/{concept1}/{concept2}{position}{concept3}</pattern>
</generator>
<generator name="Literal">
    <pattern>{text}</pattern>
</generator>

<generator name="imageURL" prefix="rmaimage">
    <pattern>{id}{type}</pattern>
</generator>

<generator name="Identifiers" prefix="rma">
    <pattern>{id}/{concept}/{reference}</pattern>
</generator>

<generator name="inlineTerms" prefix="rma">
    <pattern>{id}/{concept}/{reference}</pattern>
</generator>

<generator name="Thesaurus" prefix="rma">
    <pattern>{id}/{concept}/{reference}</pattern>
</generator>

<generator name="displayone">
    <pattern>{display1}</pattern>
</generator>

<generator name="displaytwo">
    <pattern>{display1}::{display2}</pattern>
</generator>

<generator name="displaythree">
    <pattern>{display1}::{display2}::{display3}</pattern>
</generator>

</generator_policy>