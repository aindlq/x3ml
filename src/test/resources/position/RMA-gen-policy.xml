<?xml version="1.0" encoding="UTF-8"?>
<generator_policy>

<generator name="SimpleLabel">
        <pattern>{label}</pattern>
</generator>
 
 <generator name="PersistentId" prefix="han">
        <pattern>{id}</pattern>
</generator>
    
<generator name="Priref" prefix="rma">
        <pattern>{authority}/{objectno}/{priref}</pattern>
</generator>

<generator name="Literal">
        <pattern>text()</pattern>
</generator>

<generator name="AuthorityPrirefThingThing" prefix="rma">
    <pattern>{priref}/{thing}/{thing}</pattern>
</generator>

<generator name="AuthorityPrirefThing" prefix="rma">
    <pattern>{authority}/{priref}/{thing}</pattern>
</generator>

<generator name="LinkrefThing" prefix="rma">
    <pattern>{linkref}/{thing}</pattern>
</generator>

<generator name="AuthorityPrirefThingCounter" prefix="rma">
    <pattern>{authority}/{priref}/{thing}/{counter}</pattern>
</generator>

<generator name="AuthorityPrirefThingCounter2" prefix="rma">
  <custom generatorClass="gr.forth.AuthorityPrirefThingCounter">
            <set-arg name="authority"/>
   <set-arg name="priref"/>
   <set-arg name="thing"/>
        </custom>
</generator>

<generator name="PrirefThingCounter" prefix="rma">
    <pattern>{priref}/{thing}/{counter}</pattern>
</generator>

<generator name="AuthorityPrirefCounterThing" prefix="rma">
    <pattern>{authority}/{priref}/{counter}/{thing}</pattern>
</generator>

<generator name="AuthorityPrirefThingId" prefix="rma">
    <pattern>{authority}/{priref}/{thing}/{id}</pattern>
</generator>

<generator name="AuthorityId" prefix="rma">
        <pattern>{authority}/{id}</pattern>
</generator>

<generator name="PersistentAuthorityId" prefix="han">
        <pattern>{authority}/{id}</pattern>
</generator>

<generator name="AuthorityIdCounter" prefix="rma">
        <pattern>{authority}/{id}/{counter}</pattern>
</generator>

<generator name="ICURI" prefix="han">
        <pattern>{id}</pattern>
</generator>
    
 <generator name="AuthorityDate" prefix="rma">
        <pattern>{authority}/{date}</pattern>
 </generator>
    
 <generator name="AuthorityIdThing" prefix="rma">
        <pattern>{authority}/{id}/{thing}</pattern>
 </generator>
    
  <generator name="AuthorityIdThing1Thing2" prefix="rma">
        <pattern>{authority}/{id}/{thing1}/{thing2}</pattern>
  </generator>
    
 <generator name="AuthorityIdThingCounter" prefix="rma">
        <pattern>{authority}/{id}/{thing}/{counter}</pattern>
 </generator>
 

  <generator name="AuthorityTerm" prefix="rma">
        <pattern>{authority}/{term}</pattern>
  </generator>
</generator_policy>
